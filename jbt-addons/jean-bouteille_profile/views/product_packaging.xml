<odoo>
    <record id="jean-bouteille_product_packaging_form" model="ir.ui.view">
        <field name="name">jean-bouteille.product.packaging.tree.view2</field>
        <field name="model">product.packaging</field>
        <field name="inherit_id" ref="delivery.product_packaging_delivery_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='max_weight']" position="after">
                <field name="is_pallet"/>
                <field name="gross_weight"/>
                <field name="net_weight"/>
                <field name="depth"/>
                <field name="number_of_layer"/>
                <field name="layer_height"/>
            </xpath>
        </field>
    </record>

    <record id="jean-bouteille_product_packaging_tree_view2" model="ir.ui.view">
        <field name="name">jean-bouteille.product.packaging.tree.view2</field>
        <field name="model">product.packaging</field>
        <field name="inherit_id" ref="product.product_packaging_tree_view"/>
        <field name="arch" type="xml">
            <xpath expr="//tree/field[@name='qty']" position="before">
                <field name="is_pallet"/>
            </xpath>
        </field>
    </record>
    <record id="jean-bouteille_product_packaging_form_view2" model="ir.ui.view">
        <field name="name">product.packaging.form.view2</field>
        <field name="model">product.packaging</field>
        <field name="mode">primary</field>
        <field name="inherit_id" ref="product.product_packaging_form_view2"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='qty']" position="inside">
                <field name="is_pallet"/>
                <field name="gross_weight"/>
                <field name="height"/>
                <field name="width"/>
                <field name="depth"/>
                <field name="net_weight"/>
                <field name="max_weight"/>
                <field name="number_of_layer"/>
                <field name="layer_height"/>
            </xpath>
        </field>
    </record>
</odoo>