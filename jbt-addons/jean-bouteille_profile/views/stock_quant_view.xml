<odoo>
    <data>

        <record id="view_stock_quant_tree_inherit" model="ir.ui.view">
            <field name="name">stock.quant.tree.inherit</field>
            <field name="model">stock.quant</field>
            <field name="inherit_id" ref="stock.view_stock_quant_tree"/>
            <field name="arch" type="xml">
                <field name="available_quantity" position="after">
                    <field name="reserved_qty_total" invisible="1"/>
                    <field name="total_reserved_qty" string ="Total Reserved Quantity"/>
                </field>
                <field name="quantity" position="after">
                    <field name="qty_total" invisible="1"/>
                    <field name="total_qty" string ="Total Quantity"/>
                </field>
            </field>
        </record>

    </data>
</odoo>
