<?xml version="1.0" encoding='UTF-8'?>
<odoo noupdate="1">
    <record id="jean-bouteille_stock_picking_cron" model="ir.cron">
        <field name="name">Confirmation : Import Reception/Delivery</field>
        <field name="model_id" ref="model_stock_picking"/>
        <field name="user_id" ref="base.user_root"/>
        <field name="state">code</field>
        <field name="code">model._cron_inbound_confirmation()</field>
        <field name="interval_number">1</field>
        <field name="nextcall" eval="(DateTime.now() + timedelta(hours=17)).strftime('%Y-%m-%d 00:00:00')" />
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False"/>
        <field name="priority" eval="0" />
    </record>
</odoo>